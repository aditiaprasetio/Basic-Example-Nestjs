{
  "name": "@skillacademy/exam",
  "version": "0.0.1",
  "description": "Exam service",
  "author": "Aditia Prasetio",
  "private": true,
  "publishConfig": {
    "access": "private"
  },
  "scripts": {
    "start": "nodemon",
    "start:prod": "node dist/main.js",
    "build": "rm -rf dist && tsc --p tsconfig.json -d",
    "barrels": "barrelsby --delete -d ./src --singleQuotes",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "migration:barrels": "barrelsby --delete -d ./src/migrations --singleQuotes",
    "precommit": "yarn e2e:coverage",
    "lint": "tslint -p tsconfig.json -c tslint.json \"src/**/*.ts\" \"e2e/**/*.ts\" -e \"src/migrations/**/*.ts\" \"*spec.ts\"",
    "lint:fix": "yarn lint --fix",
    "e2e": "jest --config=jest-e2e.json --forceExit --detectOpenHandles",
    "e2e:watch": "jest --watch --config=jest-e2e.json --detectOpenHandles",
    "e2e:coverage": "jest --config=jest-e2e.json --coverage --coverageDirectory=./coverage --detectOpenHandles"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@nestjs/common": "^6.5.2",
    "@nestjs/core": "^6.5.2",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/platform-express": "^6.5.2",
    "@nestjs/swagger": "^3.0.2",
    "@nestjs/typeorm": "6.1.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.0.1",
    "dotenv": "^8.0.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^1.6.5",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2",
    "typeorm": "^0.2.18"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.6.4",
    "@types/cors": "^2.8.5",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.6.2",
    "@types/uuid": "^3.4.5",
    "barrelsby": "^2.1.1",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "sqlite3": "^4.0.9",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.0.7",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3"
  }
}
